
@model List<CheeseMVC.Models.Cheese>


<h1> My Cheeses</h1>


@if (Model.Count == 0)
{
    <p>No cheeses yet</p>
}
<br />

    <table class="table">
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Type</th>
            <th>Rating</th>
            <th> </th>
        </tr>


        @foreach (Cheese cheese in Model)
        {
            <tr>
               
                <td>@cheese.Name</td>
                <td>@cheese.Description </td>
                <td>@cheese.Type</td>
                <td>@cheese.Rating</td>
                <td><a asp-controller="Cheese" asp-action="Edit" asp-route-cheeseId="@cheese.CheeseId">edit</a></td>

            </tr>

        }

    </table>
    

 

<a href="/Cheese/Remove"> Remove Options </a>
